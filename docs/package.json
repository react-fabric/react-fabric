{
  "name": "react-fabric-docs",
  "version": "0.0.1",
  "description": "Documentation for react-fabric",
  "author": "Kevin Mees <kev.mees@gmail.com",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "predeploy": "npm run lint && better-npm-run build",
    "deploy": "better-npm-run deploy",
    "lint": "eslint app"
  },
  "betterScripts": {
    "start": {
      "command": "node ./server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config ./webpack.production.config --colors --profile --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy": {
      "command": "gh-pages -d build -o gh-pages -b master"
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-standalone": "^6.10.3",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-config-xi": "^4.1.1",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.4",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.11.0",
    "highlight-loader": "git://github.com/javivelasco/highlight-loader.git#master",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "markdown-loader": "^0.1.7",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "react": "^15.0.2",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.4",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-loader": "0.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "codemirror": "^5.16.0",
    "highlight.js": "^9.3.0",
    "history": "^2.1.1",
    "json-loader": "^0.5.4",
    "react": "^15.0.2",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.0.2",
    "react-prop-types": "^0.3.1",
    "react-router": "^2.4.0",
    "react-router-scroll": "^0.2.0"
  },
  "repository": "github:kmees/react-fabric",
  "license": "MIT"
}
